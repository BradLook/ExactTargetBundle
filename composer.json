    "description": "A Symfony2 bundle for ExactTarget/FuelSDK-PHP",
    "authors": [
        {
            "name": "Bradford Dewey",
            "email": "brad.dewey@hautelook.com"
        }
    ],
    "autoload": {
        "psr-0": { "Hautelook\\ExactTargetBundle": "" }
